<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.students">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/student_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/student_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivityAddExpertise"></activity>
        <activity android:name=".MainActivityListFeedbackPage" />
        <activity android:name=".MainActivity_Feedback" />
        <activity android:name=".MainActivityCardView" />
        <activity android:name=".MainActivityManagementCardsApprov" />
        <activity android:name=".MainActivityFeedback" />
        <activity android:name=".MainActivityPageUser" />
        <activity android:name=".MainActivityRegisterTutor" />
        <activity android:name=".MainActivityRegister" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>